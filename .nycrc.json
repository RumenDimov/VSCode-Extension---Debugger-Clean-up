{
  "include": [
    "*.js",
    "!test/**",
    "!node_modules/**"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}